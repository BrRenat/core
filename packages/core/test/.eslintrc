{
  "extends": "makeomatic/typescript",
  "env": {
    "mocha": true,
    "node": true
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".ts", ".js"]
      }
    }
  },
  "rules": {
    "no-var-requires": 0,
    "semi": 0,
    "@typescript-eslint/no-var-requires": 0
  }
}
